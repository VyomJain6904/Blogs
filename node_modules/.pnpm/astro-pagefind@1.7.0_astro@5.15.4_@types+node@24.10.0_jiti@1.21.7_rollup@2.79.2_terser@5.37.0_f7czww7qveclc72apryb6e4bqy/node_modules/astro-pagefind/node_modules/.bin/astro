#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jain/Study/Blogs/node_modules/.pnpm/astro@5.15.4_@types+node@24.10.0_jiti@1.21.7_rollup@2.79.2_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules/astro/node_modules:/home/jain/Study/Blogs/node_modules/.pnpm/astro@5.15.4_@types+node@24.10.0_jiti@1.21.7_rollup@2.79.2_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules:/home/jain/Study/Blogs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jain/Study/Blogs/node_modules/.pnpm/astro@5.15.4_@types+node@24.10.0_jiti@1.21.7_rollup@2.79.2_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules/astro/node_modules:/home/jain/Study/Blogs/node_modules/.pnpm/astro@5.15.4_@types+node@24.10.0_jiti@1.21.7_rollup@2.79.2_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules:/home/jain/Study/Blogs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../astro@5.15.4_@types+node@24.10.0_jiti@1.21.7_rollup@2.79.2_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../astro@5.15.4_@types+node@24.10.0_jiti@1.21.7_rollup@2.79.2_terser@5.37.0_typescript@5.7.3_yaml@2.7.0/node_modules/astro/astro.js" "$@"
fi
